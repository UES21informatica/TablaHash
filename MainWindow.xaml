<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="Nombre" HorizontalAlignment="Left" Height="27" Margin="10,10,0,0" TextWrapping="Wrap" Text="Nombre" VerticalAlignment="Top" Width="221" TextChanged="TextBox_TextChanged"/>
        <Button x:Name="Agregar" Content="Agregar" HorizontalAlignment="Left" Height="29" Margin="236,23,0,0" VerticalAlignment="Top" Width="91" Click="Button_Click"/>
        <TextBox x:Name="Salida" HorizontalAlignment="Left" Height="130" Margin="44,157,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="451" TextChanged="TextBox_TextChanged_1"/>
        <TextBox x:Name="DNI" HorizontalAlignment="Left" Height="27" Margin="10,42,0,0" TextWrapping="Wrap" Text="DNI" VerticalAlignment="Top" Width="221" TextChanged="TextBox_TextChanged"/>

    </Grid>
</Window>
